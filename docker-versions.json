{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": ["registry.k8s.io/external-dns/external-dns"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/kubernetes-sigs/external-dns"
    },
    {
      "matchPackageNames": ["kubernetesui/dashboard"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/kubernetes/dashboard"
    },
    {
      "matchPackageNames": ["kubernetesui/metrics-scraper"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/kubernetes-sigs/dashboard-metrics-scraper"
    },
    {
      "matchPackageNames": ["k8s.gcr.io/descheduler/descheduler"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/kubernetes-sigs/descheduler"
    },
    {
      "matchPackageNames": ["registry.k8s.io/metrics-server/metrics-server"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/kubernetes-sigs/metrics-server"
    },
    {
      "matchPackageNames": ["public.ecr.aws/aws-ec2/aws-node-termination-handler"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/aws/aws-node-termination-handler/"
    },
    {
      "matchPackageNames": ["nginx"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/nginxinc/docker-nginx"
    },
    {
      "matchPackageNames": ["hashicorp/vault"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/hashicorp/vault"
    },
    {
      "matchPackageNames": ["hashicorp/packer"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/hashicorp/packer"
    },
    {
      "matchPackageNames": ["telegraf"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/influxdata/telegraf"
    },
    {
      "matchPackagePatterns": ["grafana/grafana"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/grafana/grafana"
    },
    {
      "matchPackageNames": [
        "prometheus/prometheus",
        "quay.io/prometheus/prometheus"
      ],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/prometheus/prometheus"
    }
  ]
}
